import java.util.Scanner;

public class ubesxkz {
	public static void main(String[] args){
		double c = 11;
		double b = 250;
		Scanner scan = new Scanner(System.in);
		System.out.println("How many are you going to sell");
		double nummy = scan.nextDouble();
		System.out.println("What is the selling price of one quilt?");
		double p = scan.nextDouble();
		double profit = (nummy * p) - b - (nummy * c);
		System.out.println(profit + " dollars");
		scan.close(); 
		
	}

}
